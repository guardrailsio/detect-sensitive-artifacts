{
  "name": "detect-sensitive-artifacts",
  "version": "1.0.0",
  "private": true,
  "allowFor": {
    "providers": {
      "github": ["guardrailsio", "streichsbaer"],
      "gitlab": []
    }
  },
  "runForLanguage": "general",
  "rules": {
    "sensitive": {
      "grId": "GR0004",
      "enable": true
    },
    "secrets": {
      "grId": "GR0004",
      "enable": true
    }
  }
}
